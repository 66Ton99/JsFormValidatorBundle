<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="fp_js_form_validator.twig_extension.class">Fp\JsFormValidatorBundle\Twig\Extension\JsFormValidatorExtension</parameter>
    </parameters>

    <services>
        <service id="fp_js_form_validator.twig_extension" class="%fp_js_form_validator.twig_extension.class%">
            <argument type="service" id="validator" />
            <argument type="service" id="translator" />
            <argument>%fp_js_form_validator.translation_domain%</argument>
            <tag name="twig.extension" />
        </service>
    </services>
</container>
